{
    "swagger": "2.0",
    "info": {
        "contact": {}
    },
    "paths": {
        "/get-presets": {
            "get": {
                "description": "알람조회시 호출 (10개씩)",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "알람"
                ],
                "summary": "알람조회",
                "parameters": [
                    {
                        "type": "integer",
                        "description": "페이지 번호 default 0",
                        "name": "page",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "성공시 200 반환",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/dto.AlarmResponse"
                            }
                        }
                    },
                    "400": {
                        "description": "요청 처리 실패시 오류 메시지 반환",
                        "schema": {
                            "$ref": "#/definitions/dto.ErrorResponse"
                        }
                    },
                    "500": {
                        "description": "요청 처리 실패시 오류 메시지 반환",
                        "schema": {
                            "$ref": "#/definitions/dto.ErrorResponse"
                        }
                    }
                }
            }
        },
        "/remove-alarm/{id}": {
            "post": {
                "description": "알람삭제시 호출",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "알람"
                ],
                "summary": "알람삭제",
                "parameters": [
                    {
                        "type": "string",
                        "description": "알람ID",
                        "name": "id",
                        "in": "path"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "성공시 200 반환",
                        "schema": {
                            "$ref": "#/definitions/dto.BasicResponse"
                        }
                    },
                    "400": {
                        "description": "요청 처리 실패시 오류 메시지 반환",
                        "schema": {
                            "$ref": "#/definitions/dto.ErrorResponse"
                        }
                    },
                    "500": {
                        "description": "요청 처리 실패시 오류 메시지 반환",
                        "schema": {
                            "$ref": "#/definitions/dto.ErrorResponse"
                        }
                    }
                }
            }
        },
        "/save-alarm": {
            "post": {
                "description": "알람생성시 id생략 / 알람수정시 id 포함",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "알람"
                ],
                "summary": "알람생성/수정",
                "parameters": [
                    {
                        "description": "요청 DTO - 알람데이터",
                        "name": "request",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/dto.AlarmRequest"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "성공시 200 반환",
                        "schema": {
                            "$ref": "#/definitions/dto.BasicResponse"
                        }
                    },
                    "400": {
                        "description": "요청 처리 실패시 오류 메시지 반환",
                        "schema": {
                            "$ref": "#/definitions/dto.ErrorResponse"
                        }
                    },
                    "500": {
                        "description": "요청 처리 실패시 오류 메시지 반환",
                        "schema": {
                            "$ref": "#/definitions/dto.ErrorResponse"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "dto.AlarmRequest": {
            "type": "object",
            "properties": {
                "body": {
                    "type": "string"
                },
                "end_at": {
                    "type": "string",
                    "example": "yyyy-mm-dd"
                },
                "id": {
                    "type": "integer"
                },
                "start_at": {
                    "type": "string",
                    "example": "yyyy-mm-dd"
                },
                "timestamp": {
                    "type": "string",
                    "example": "HH:mm"
                },
                "type": {
                    "type": "string"
                },
                "week": {
                    "type": "string",
                    "example": "0,4,6 (sunday:0,...)"
                }
            }
        },
        "dto.AlarmResponse": {
            "type": "object",
            "properties": {
                "body": {
                    "type": "string"
                },
                "created": {
                    "type": "string"
                },
                "end_at": {
                    "type": "string",
                    "example": "yyyy-mm-dd"
                },
                "id": {
                    "type": "integer"
                },
                "start_at": {
                    "type": "string",
                    "example": "yyyy-mm-dd"
                },
                "timestamp": {
                    "type": "string",
                    "example": "HH:mm"
                },
                "type": {
                    "type": "string"
                },
                "updated": {
                    "type": "string"
                },
                "week": {
                    "type": "string",
                    "example": "0,4,6 (sunday:0,...)"
                }
            }
        },
        "dto.BasicResponse": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "string"
                }
            }
        },
        "dto.ErrorResponse": {
            "type": "object",
            "properties": {
                "err": {
                    "type": "string"
                }
            }
        }
    }
}