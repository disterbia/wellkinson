{
    "swagger": "2.0",
    "info": {
        "contact": {}
    },
    "paths": {
        "/get-vocal-scores": {
            "get": {
                "description": "발성 점수 조회시 호출",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "발성 /vocal"
                ],
                "summary": "발성 점수 조회",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Bearer {jwt_token}",
                        "name": "Authorization",
                        "in": "header",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "시작날짜 yyyy-mm-dd",
                        "name": "start_date",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "종료날짜 yyyy-mm-dd",
                        "name": "end_date",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "표정검사 점수 정보",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/dto.VocalScoreResponse"
                            }
                        }
                    },
                    "400": {
                        "description": "요청 처리 실패시 오류 메시지 반환",
                        "schema": {
                            "$ref": "#/definitions/dto.ErrorResponse"
                        }
                    },
                    "500": {
                        "description": "요청 처리 실패시 오류 메시지 반환",
                        "schema": {
                            "$ref": "#/definitions/dto.ErrorResponse"
                        }
                    }
                }
            }
        },
        "/get-voice-tables": {
            "get": {
                "description": "발성운동 단어 조회시 호출",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "발성 /vocal"
                ],
                "summary": "발성운동 단어 조회",
                "responses": {
                    "200": {
                        "description": "발성운동 단어 데이터",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/dto.VoiceWordResponse"
                            }
                        }
                    },
                    "400": {
                        "description": "요청 처리 실패시 오류 메시지 반환",
                        "schema": {
                            "$ref": "#/definitions/dto.ErrorResponse"
                        }
                    },
                    "500": {
                        "description": "요청 처리 실패시 오류 메시지 반환",
                        "schema": {
                            "$ref": "#/definitions/dto.ErrorResponse"
                        }
                    }
                }
            }
        },
        "/save-vocals": {
            "post": {
                "description": "발성 검사 완료 후 호출",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "발성 /vocal"
                ],
                "summary": "발성 점수 저장",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Bearer {jwt_token}",
                        "name": "Authorization",
                        "in": "header",
                        "required": true
                    },
                    {
                        "description": "요청 DTO - 발성검사 데이터 ( type: 1:a 2:e 3:i 4:o 5:u )",
                        "name": "request",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/dto.VocalScoreRequest"
                            }
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "성공시 200 반환",
                        "schema": {
                            "$ref": "#/definitions/dto.BasicResponse"
                        }
                    },
                    "400": {
                        "description": "요청 처리 실패시 오류 메시지 반환",
                        "schema": {
                            "$ref": "#/definitions/dto.ErrorResponse"
                        }
                    },
                    "500": {
                        "description": "요청 처리 실패시 오류 메시지 반환",
                        "schema": {
                            "$ref": "#/definitions/dto.ErrorResponse"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "dto.BasicResponse": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "string"
                }
            }
        },
        "dto.ErrorResponse": {
            "type": "object",
            "properties": {
                "err": {
                    "type": "string"
                }
            }
        },
        "dto.VocalScoreRequest": {
            "type": "object",
            "properties": {
                "score": {
                    "type": "integer"
                },
                "type": {
                    "type": "integer"
                }
            }
        },
        "dto.VocalScoreResponse": {
            "type": "object",
            "properties": {
                "created": {
                    "type": "string",
                    "example": "YYYY-mm-ddTHH:mm:ssZ (ISO8601) "
                },
                "score": {
                    "type": "integer"
                },
                "type": {
                    "type": "integer"
                },
                "updated": {
                    "type": "string",
                    "example": "YYYY-mm-ddTHH:mm:ssZ (ISO8601) "
                }
            }
        },
        "dto.VoiceWordResponse": {
            "type": "object",
            "properties": {
                "title": {
                    "type": "string"
                },
                "type": {
                    "type": "integer"
                }
            }
        }
    }
}