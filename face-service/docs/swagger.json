        "/get-face-scores": {
            "get": {
                "description": "표정 점수 조회시 호출",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "표정 /face"
                ],
                "summary": "표정 점수 조회",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Bearer {jwt_token}",
                        "name": "Authorization",
                        "in": "header",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "시작날짜 yyyy-mm-dd",
                        "name": "start_date",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "종료날짜 yyyy-mm-dd",
                        "name": "end_date",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "표정검사 점수 정보",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/dto.FaceScoreResponse"
                            }
                        }
                    },
                    "400": {
                        "description": "요청 처리 실패시 오류 메시지 반환",
